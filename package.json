{
  "name": "elegant-seo-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:old": "concurrently --kill-others \"pnpm scss:critical --watch\" \"pnpm scss:non-critical --watch\" \"vite\"",
    "build:old": "pnpm scss:critical; pnpm scss:non-critical; pnpm build",
    "scss:critical": "sass --style=compressed css/critical.scss:html/partials/css-raw-critical.hbs",
    "scss:non-critical": "sass --style=compressed css/non-critical.scss:public/css/non-critical.css"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "sass": "^1.62.1",
    "svelte": "^3.59.1",
    "vite": "^4.3.2",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-ssr": "^0.4.130"
  }
}
